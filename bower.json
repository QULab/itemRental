{
  "name": "libnode",
  "version": "0.0.1",
  "authors": [
    "chohner <mail@chohner.com>"
  ],
  "moduleType": [
    "node"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "2.x.x",
    "bootstrap": "~3.x.x",
    "datatables": "~1.x.x",
    "html5sortable": "*",
    "papaparse": "4.1.x",
    "introjs": "~1.x.x"
  },
  "bowerMove": {
    "jquery": {
      "dist/jquery.min.js": "public/js/",
      "dist/jquery.min.map": "public/js/"
    },
    "bootstrap": {
      "less/[!v]*.less": "src/bootstrap_style/",
      "less/mixins/*.less": "src/bootstrap_style/mixins/",
      "dist/fonts/*": "public/fonts/",
      "dist/js/bootstrap.min.js": "public/js/"
    },
    "datatables": {
      "media/css/jquery.dataTables.min.css": "public/css/",
      "media/images/*.png": "public/images/",
      "media/js/jquery.dataTables.min.js": "public/js/"
    },
    "html5sortable": {
      "jquery.sortable.min.js": "public/js/"
    },
    "papaparse": {
      "papaparse.min.js": "public/js/"
    },
    "introjs": {
      "minified/introjs.min.css": "public/css",
      "minified/intro.min.js": "public/js"
    }
  }
}
